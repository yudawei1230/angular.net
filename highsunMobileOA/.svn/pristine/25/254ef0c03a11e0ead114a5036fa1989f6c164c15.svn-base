* {
	margin: 0px;
	padding: 0px;
	font-family: "微软雅黑", Arial, Helvetica, sans-serif;
}
body {
	margin: 0px;
	padding: 0px;
	display: block;
	width: 100%;
	height: 100%;
	background: #dddddd;
}
html, body {
	display: block;
	width: 100%;
	height: 100%
}
ul {
	list-style: none;
}
img {
	border: none;
	vertical-align: middle;
}
i, em {
	font-style: normal;
}
/******************/
.greetBg {
	width: 100%;
 height: 100%;  max-width:640px;
	margin: 0px auto;
	background: url(../images/bg.jpg) no-repeat center center;
}
